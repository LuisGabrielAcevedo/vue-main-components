    "material-design-icons-iconfont": "~4.0.2",
    "lodash": "~4.17.11",
    "vuetify": "~1.3.8"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~3.1.1",
    "@vue/cli-plugin-typescript": "~3.6.0",
    "@types/lodash": "~4.14.120",
    "@vue/cli-service": "~3.6.0",
    "typescript": "~3.4.5",
    "vue-template-compiler": "~2.5.17"
